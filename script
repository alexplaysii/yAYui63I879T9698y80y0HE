-- Scripts:

local function NKYMA_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.temp.LocalScript.Disabled = not script.Parent.Parent.temp.LocalScript.Disabled
		script.Parent.Parent.temp.Text = script.Parent.Parent.temp.LocalScript.Disabled and "0FF" or "..."
	end)
end
coroutine.wrap(NKYMA_fake_script)()
local function YXTGMN_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		fireclickdetector(workspace.Core.Pump2Buttons["2"].ClickDetector)
		fireclickdetector(workspace.Core.Pump1Buttons["2"].ClickDetector)
		fireclickdetector(workspace.Core.FanButtons["4"].ClickDetector)
	end)
	
	
	
	
	
	
end
coroutine.wrap(YXTGMN_fake_script)()
-- temp.LocalScript is disabled.
local function BGVEAZ_fake_script() -- Frame.Dragify 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
		dragToggle = nil
		dragSpeed = 0.15
		dragInput = nil
		dragStart = nil
		dragPos = nil
		function updateInput(input)
			Delta = input.Position - dragStart
			Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.15), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	dragify(script.Parent)
	
end
coroutine.wrap(BGVEAZ_fake_script)()
local function GSSJPAT_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	script.Parent.MouseButton1Click:Connect(function()
		fireclickdetector(workspace.Core.Pump2Buttons["0"].ClickDetector)
		fireclickdetector(workspace.Core.Pump1Buttons["0"].ClickDetector)
		fireclickdetector(workspace.Core.FanButtons["0"].ClickDetector)
	end)
	
	
	
	
	
	
end
coroutine.wrap(GSSJPAT_fake_script)()
local function BUJL_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	script.Parent.MouseButton1Click:Connect(function()
		local lastpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		for i,v in ipairs(workspace:GetDescendants()) do
			if v.Name == "Center" and v.Parent.Name == "EnergyCore" and not v:FindFirstAncestor("Bunker") then
				game.Players.LocalPlayer.Character:PivotTo(v.CFrame)
				task.wait(.1)
				for i2,v2 in ipairs(workspace.Bunker.Cores:GetDescendants()) do
					if v2.Name == "Hitbox" then
						game.Players.LocalPlayer.Character:PivotTo(v2.CFrame)
						task.wait(.1)
					end
				end
			end
		end
		game.Players.LocalPlayer.Character:PivotTo(lastpos)
	end)
	
	
	
	
	
end
coroutine.wrap(BUJL_fake_script)()
