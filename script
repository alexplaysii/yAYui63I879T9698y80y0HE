-- Scripts:

local function DZOG_fake_script() -- Frame.scripty 
	local script = Instance.new('LocalScript', Frame)

	--local script = Instance.new('LocalScript', Frame)

	local TEMPNO = false

	local con 
	local p1con
	local p2con
	local fancon

	local function changedthevalue(val)
		if TEMPNO == true then return end
		local coretemp = math.floor(val)

		if coretemp < 1000 then
			script.Parent.tempwarning.Text = "âœ… Normal Temperature"
			script.Parent.tempwarning.TextColor3 = Color3.fromRGB(0, 150, 0)
			script.Parent.temp.TextColor3 = Color3.fromRGB(0, 150, 0)
		elseif coretemp >= 1000 and coretemp < 1500 then
			script.Parent.tempwarning.Text = "It's getting a bit hot!"
			script.Parent.tempwarning.TextColor3 = Color3.fromRGB(255, 100, 0)
			script.Parent.temp.TextColor3 = Color3.fromRGB(255, 100, 0)
		elseif coretemp >= 1500 and coretemp < 1750 then
			script.Parent.tempwarning.Text = "High Temperature!"
			script.Parent.tempwarning.TextColor3 = Color3.fromRGB(255, 50, 0)
			script.Parent.temp.TextColor3 = Color3.fromRGB(255, 50, 0)
		elseif coretemp >= 1750 and coretemp < 2000 then
			script.Parent.tempwarning.Text = "ðŸš¨ Very High Temperature!"
			script.Parent.tempwarning.TextColor3 = Color3.fromRGB(255, 0, 0)
			script.Parent.temp.TextColor3 = Color3.fromRGB(255, 0, 0)
		elseif coretemp >= 2000 then
			script.Parent.tempwarning.Text = "WARNING: CORE CRITICAL"
			script.Parent.tempwarning.TextColor3 = Color3.fromRGB(150, 0, 0)
			script.Parent.temp.TextColor3 = Color3.fromRGB(150, 0, 0)
		end

		script.Parent.temp.Text = coretemp

	end

	local function thething2()
		while task.wait() do
			if TEMPNO == true then 

				script.Parent.P1Stats.Text = "Disabled"
				script.Parent.P1Stats.TextColor3 = Color3.fromRGB(255, 0, 0)
				script.Parent.P2Stats.Text = "Disabled"
				script.Parent.P2Stats.TextColor3 = Color3.fromRGB(255, 0, 0)
				script.Parent.FanSpd.Text = "Disabled"
				script.Parent.FanSpd.TextColor3 = Color3.fromRGB(255, 0, 0)

				script.Parent.P1TankLeft.Text = "N/A"
				script.Parent.P2TankLeft.Text = "N/A"
				break 
			end

			if tonumber(string.format("%.3f", tostring(workspace.Core.Pump2.Spinny.MotorPart.Motor.MaxVelocity))) == 0.1 then
				script.Parent.P1Stats.Text = "2"
				script.Parent.P1Stats.TextColor3 = Color3.fromRGB(0, 255, 0)
			elseif tonumber(string.format("%.3f", tostring(workspace.Core.Pump2.Spinny.MotorPart.Motor.MaxVelocity))) == 0.05 then
				script.Parent.P1Stats.Text = "1"
				script.Parent.P1Stats.TextColor3 = Color3.fromRGB(0, 255, 0)
			elseif workspace.Core.Pump1.Spinny.MotorPart.Motor.MaxVelocity > 0 then
				script.Parent.P1Stats.TextColor3 = Color3.fromRGB(255, 255, 0)
				script.Parent.P1Stats.Text = "..."
			elseif workspace.Core.Pump1.Spinny.MotorPart.Motor.MaxVelocity == 0 then
				script.Parent.P1Stats.TextColor3 = Color3.fromRGB(255, 0, 0)
				script.Parent.P1Stats.Text = "OFF"
			end

			if tonumber(string.format("%.3f", tostring(workspace.Core.Pump2.Spinny.MotorPart.Motor.MaxVelocity))) == 0.1 then
				script.Parent.P2Stats.Text = "2"
				script.Parent.P2Stats.TextColor3 = Color3.fromRGB(0, 255, 0)
			elseif tonumber(string.format("%.3f", tostring(workspace.Core.Pump2.Spinny.MotorPart.Motor.MaxVelocity))) == 0.05 then
				script.Parent.P2Stats.Text = "1"
				script.Parent.P2Stats.TextColor3 = Color3.fromRGB(0, 255, 0)
			elseif workspace.Core.Pump2.Spinny.MotorPart.Motor.MaxVelocity > 0 then
				script.Parent.P2Stats.TextColor3 = Color3.fromRGB(255, 255, 0)
				script.Parent.P2Stats.Text = "..."
			elseif workspace.Core.Pump2.Spinny.MotorPart.Motor.MaxVelocity == 0 then
				script.Parent.P2Stats.TextColor3 = Color3.fromRGB(255, 0, 0)
				script.Parent.P2Stats.Text = "OFF"
			end

			if  tonumber(string.format("%.3f", tostring(workspace.Core.MainFan.Centre.Motor.MaxVelocity))) == 0.05 then
				script.Parent.FanSpd.Text = "4"
				script.Parent.FanSpd.TextColor3 = Color3.fromRGB(0, 255, 0)
			elseif tonumber(string.format("%.3f", tostring(workspace.Core.MainFan.Centre.Motor.MaxVelocity))) == 0.038 then
				script.Parent.FanSpd.Text = "3"
				script.Parent.FanSpd.TextColor3 = Color3.fromRGB(0, 255, 0)
			elseif tonumber(string.format("%.3f", tostring(workspace.Core.MainFan.Centre.Motor.MaxVelocity))) == 0.025 then
				script.Parent.FanSpd.Text = "2"
				script.Parent.FanSpd.TextColor3 = Color3.fromRGB(0, 255, 0)
			elseif tonumber(string.format("%.3f", tostring(workspace.Core.MainFan.Centre.Motor.MaxVelocity))) == 0.013 then
				script.Parent.FanSpd.Text = "1"
				script.Parent.FanSpd.TextColor3 = Color3.fromRGB(0, 255, 0)
			elseif workspace.Core.MainFan.Centre.Motor.MaxVelocity > 0 then
				script.Parent.FanSpd.TextColor3 = Color3.fromRGB(255, 255, 0)
				script.Parent.FanSpd.Text = "..."
			elseif workspace.Core.MainFan.Centre.Motor.MaxVelocity == 0 then
				script.Parent.FanSpd.Text = "OFF"
				script.Parent.FanSpd.TextColor3 = Color3.fromRGB(255, 0, 0)
			end

			script.Parent.P1TankLeft.Text = math.floor(workspace.GameState.Core.Coolant1.Value).."%"
			script.Parent.P2TankLeft.Text = math.floor(workspace.GameState.Core.Coolant2.Value).."%"

		end
	end

	task.spawn(function()
		local UIS = game:GetService("UserInputService")
		function dragify(Frame)
			dragToggle = nil
			dragSpeed = 0.15
			dragInput = nil
			dragStart = nil
			dragPos = nil
			function updateInput(input)
				Delta = input.Position - dragStart
				Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
				game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.15), {Position = Position}):Play()
			end
			Frame.InputBegan:Connect(function(input)
				if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
					dragToggle = true
					dragStart = input.Position
					startPos = Frame.Position
					input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End then
							dragToggle = false
						end
					end)
				end
			end)
			Frame.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
					dragInput = input
				end
			end)
			game:GetService("UserInputService").InputChanged:Connect(function(input)
				if input == dragInput and dragToggle then
					updateInput(input)
				end
			end)
		end
		dragify(script.Parent)
	end)

	script.Parent.Temp.MouseButton1Click:Connect(function()
		if TEMPNO == true then
			TEMPNO = false
			script.Parent.temp.Text = "..."
			script.Parent.temp.TextColor3 = Color3.fromRGB(0, 0, 0)
			script.Parent.tempwarning.Text =  "..."
			script.Parent.temp.TextColor3 = Color3.fromRGB(0, 0, 0)
			con = workspace.GameState.Core.Temperature.Changed:Connect(changedthevalue)
			task.spawn(thething2)
		elseif TEMPNO == false then
			con:Disconnect()
			script.Parent.temp.Text = "OFF"
			script.Parent.temp.TextColor3 = Color3.fromRGB(0, 0, 0)
			script.Parent.tempwarning.Text = "OFF"
			script.Parent.temp.TextColor3 = Color3.fromRGB(0, 0, 0)
			TEMPNO = true
		end
	end)

	script.Parent.Battery.MouseButton1Click:Connect(function()
		local lastpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		for i,v in ipairs(workspace:GetDescendants()) do
			if v.Name == "Center" and v.Parent.Name == "EnergyCore" and not v:FindFirstAncestor("Bunker") then
				game.Players.LocalPlayer.Character:PivotTo(v.CFrame)
				task.wait(.1)
				for i2,v2 in ipairs(workspace.Bunker.Cores:GetDescendants()) do
					if v2.Name == "Hitbox" then
						game.Players.LocalPlayer.Character:PivotTo(v2.CFrame)
						task.wait(.1)
					end
				end
			end
		end
		game.Players.LocalPlayer.Character:PivotTo(lastpos)
	end)

	script.Parent.Cool.MouseButton1Click:Connect(function()
		fireclickdetector(workspace.Core.Pump2Buttons["2"].ClickDetector)
		fireclickdetector(workspace.Core.Pump1Buttons["2"].ClickDetector)
		fireclickdetector(workspace.Core.FanButtons["4"].ClickDetector)
	end)

	script.Parent.NotCool.MouseButton1Click:Connect(function()
		fireclickdetector(workspace.Core.Pump2Buttons["0"].ClickDetector)
		fireclickdetector(workspace.Core.Pump1Buttons["0"].ClickDetector)
		fireclickdetector(workspace.Core.FanButtons["0"].ClickDetector)
	end)

	local coretemp = math.floor(workspace.GameState.Core.Temperature.Value)

	if coretemp < 1000 then
		script.Parent.tempwarning.Text = "âœ… Normal Temperature"
		script.Parent.tempwarning.TextColor3 = Color3.fromRGB(0, 150, 0)
		script.Parent.temp.TextColor3 = Color3.fromRGB(0, 150, 0)
	elseif coretemp >= 1000 and coretemp < 1500 then
		script.Parent.tempwarning.Text = "It's getting a bit hot!"
		script.Parent.tempwarning.TextColor3 = Color3.fromRGB(255, 100, 0)
		script.Parent.temp.TextColor3 = Color3.fromRGB(255, 100, 0)
	elseif coretemp >= 1500 and coretemp < 1750 then
		script.Parent.tempwarning.Text = "High Temperature!"
		script.Parent.tempwarning.TextColor3 = Color3.fromRGB(255, 50, 0)
		script.Parent.temp.TextColor3 = Color3.fromRGB(255, 50, 0)
	elseif coretemp >= 1750 and coretemp < 2000 then
		script.Parent.tempwarning.Text = "ðŸš¨ Very High Temperature!"
		script.Parent.tempwarning.TextColor3 = Color3.fromRGB(255, 0, 0)
		script.Parent.temp.TextColor3 = Color3.fromRGB(255, 0, 0)
	elseif coretemp >= 2000 then
		script.Parent.tempwarning.Text = "WARNING: CORE CRITICAL"
		script.Parent.tempwarning.TextColor3 = Color3.fromRGB(150, 0, 0)
		script.Parent.temp.TextColor3 = Color3.fromRGB(150, 0, 0)
	end

	script.Parent.temp.Text = coretemp

	con = workspace.GameState.Core.Temperature.Changed:Connect(changedthevalue)
	task.spawn(thething2)
end
coroutine.wrap(DZOG_fake_script)()

end
